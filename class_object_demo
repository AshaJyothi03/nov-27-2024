class arithmetic_op:
    def sum(self,a,b):
        return a+b
    def mul(self,x,y):
        return x*y
    def sub(self,p,q):
        return p-q
    def mod(self,a,b):
        return a%b
    def div(self,c,d):
        return c/d
    def fact(self,z):
        res=1
        for i in range(1,z+1):
            res=res*i
        return res

    def find_even_no(self,list1):
        for i in list1:
            if i%2==0:
                print(i,end=" ")
